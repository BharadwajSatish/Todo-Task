["<template>\n  <div id=\"app\">\n    <Header />\n    <AddTodo v-on:add-todo=\"addTodo\" />\n    <Todos v-bind:todos=\"todos\" v-on:del-todo=\"deleteTodo\" />\n    <Footer />\n  </div>\n</template>\n\n<script>\nimport Header from './components/layout/Header';\nimport Footer from './components/layout/Footer';\nimport Todos from './components/Todos';\nimport AddTodo from './components/AddTodo';\n\nexport default {\n  name: 'App',\n  components:{\n    Header,\n    Todos,\n    Footer,\n    AddTodo\n    },\n  data(){\n    return {\n      todos:[\n        {\n          id:1,\n          title:\"Buy milk\",\n          completed:true\n        },\n        {\n          id:2,\n          title:\"Go to Dmart \",\n          completed:false\n        },\n        {\n          id:3,\n          title:\"Learn Vue.js\",\n          completed:false\n        }\n      ]\n    }\n  },\n  methods:{\n    deleteTodo(id){\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n    addTodo(newTodo){\n      this.todos = [...this.todos, newTodo];\n\n    }\n  }\n  }\n  \n</script>\n\n<style>\n  *{\n    box-sizing: border-box;\n    margin: 0;\n    padding: 0;\n  }\n  body{\n    font-family: sans-serif;\n  }\n  .btn{\n    display: inline-block;\n    border: none;\n    background: #555;\n    color: #fff;\n    padding: 7px 20px;\n    cursor: pointer;\n  }\n  .btn:hover{\n    background: green;\n  }\n</style>\n"]}]}